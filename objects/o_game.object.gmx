        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Next round trigger and firing lazors

var chance_to_fire = 0;
ufo_present = irandom(10);

if (global.extra_life_counter &gt;= 1000) {
    global.remaining += 1;
    global.extra_life_counter -= 1000;
    with(o_lives) {
        instance_destroy();
    }
    for (i = 0; i &lt; global.remaining; i += 1) {
        instance_create(8+i*12, 150, o_lives); //displays remaining lives
    }
}

if (global.game_on = true) {

    if (global.ship_destroyed = false) {
        with(o_ufok_parent) {
            if (y &gt;= 128) {
            //if aliens reach bottom of the screen, game over
                with(o_stateczek) {
                    instance_destroy();
                    global.ship_destroyed = true;
                    global.remaining = -10;
                    audio_play_sound(sfx_game_over, 1, false);
                }
            }
            chance_to_fire = irandom(40*global.difficulty*instance_number(o_ufok_parent));
            if (chance_to_fire = 1) {
                instance_create(x, y+4, o_wrogi_lazor);
            }
            if (irandom(5000) = 1  &amp;&amp; instance_number(o_duze_ufo) = 0) { 
            //Create a big ufo with small chance if none present
                global.big_ufo_direction = choose(-2, 2);
                if (global.big_ufo_direction = 2) {
                    instance_create(8, 8, o_duze_ufo);
                }
                else {
                    instance_create(248, 8, o_duze_ufo);
                }
            }
        }
    
        if (instance_number(o_ufok_parent) = 0 &amp;&amp; next_round = 0) {
            next_round = 1;
            alarm[ROUND_DELAY] = round_delay;
        }
    }
    else {
        with(o_wrogi_lazor) {
            instance_destroy();
        }
        if (global.wait_for_respawn = false) {
            alarm[RESPAWN] = 60;
            global.wait_for_respawn = true;
        }
    }
}
else {
    if (keyboard_check(vk_anykey)) {
        global.game_on = true;
        with(o_title) {
            instance_destroy();
        }
        with(o_credits) {
            instance_destroy();
        }       
    }
}

