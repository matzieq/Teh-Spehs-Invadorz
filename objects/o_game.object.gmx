            <string>/// move ufoks
alarm[UFOK_MOVE] = ceil(ufok_speed*global.difficulty);
global.ufok_to_move = 1;
var changedir = 0;

if (instance_number(o_ufok_parent) &gt; 0) {
    ufok_speed = instance_number(o_ufok_parent);
}

if (global.ship_destroyed = false) {
    with(o_ufok_parent) {

        x += global.ufok_dir;

        if (image_index = 1) {
            image_index = 2;
            if (instance_number(o_duze_ufo) = 0) {
                audio_play_sound(sfx_descend_1, 1, false);
            }
        }
        else {
            image_index = 1;
            if (instance_number(o_duze_ufo) = 0) {
                audio_play_sound(sfx_descend_2, 2, false);
            }
        }

        if ((x &lt; 5 || x &gt; room_width-5) &amp;&amp; changedir = 0) {       
            changedir = 1;                          
        }
    }
}

if (changedir = 1) {
    global.ufok_dir = -global.ufok_dir;
    global.ufok_y += UFOK_VERT;
    changedir = 0;
}
